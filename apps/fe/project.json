{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/fe",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/fe",
        "outputPath": "dist/apps/fe",
        "postcssConfig": "apps/fe/postcss.config.js"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/fe"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "fe:build",
        "dev": true,
        "port": 3000,
        "buildLibsFromSource": true
      },
      "configurations": {
        "development": {
          "buildTarget": "fe:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "fe:build:production",
          "dev": false
        }
      }
    },
    "gql": {
      "executor": "@dnrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/fe",
        "command": "dotenv -- graphql-codegen --config codegen.yml"
      }
    },
    "gql-watch": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/fe",
        "command": "dotenv -- graphql-codegen --config codegen.yml --watch --silent"
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "fe:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/fe"],
      "options": {
        "jestConfig": "apps/fe/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/fe/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
